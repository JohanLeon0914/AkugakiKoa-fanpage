---
import { Carrousel } from '../components/Carrousel';
import Lore from '../components/Lore.astro';
import Profile from '../components/Profile.astro';
import Redes from '../components/Enlaces.astro';
import Layout from '../layouts/Layout.astro';
import Footer from '../components/footer.astro';
import Fanarts from '../components/Fanarts.astro';
import LoreAndFanarts from '../components/LoreAndFanarts.astro';
---

<Layout title="Akugaki Koa - Fan Page">
	<main>
		<div id="inicio" class="pt-[50px]">
			<Carrousel client:load />
		</div>

		<div id="perfil">
			<Profile />
		</div>

		<LoreAndFanarts />

		<div id="enlaces">
			<Redes />
		</div>

		<Footer />
	</main>
</Layout>

